{
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/admin",
            "destination": "/index.html"
        },
        {
            "source": "/admin/(.*)",
            "destination": "/index.html"
        },
        {
            "source": "/((?!assets|manifest.webmanifest|sw.js|workbox-.*\\.js|pwa-.*\\.png|.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json)).*)",
            "destination": "/index.html"
        }
    ]
}